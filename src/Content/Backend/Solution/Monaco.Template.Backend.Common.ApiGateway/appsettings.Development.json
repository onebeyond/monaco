{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Information",
				"System": "Information"
			}
		},
		"WriteTo": [
			{ "Name": "Debug" },
			{ "Name": "Console" },
			{
				"Name": "File",
				"Args": {
					"path": "logs/log.txt",
					"rollingInterval": "Day",
					"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
				}
			}
		],
		"Enrich": [ "FromLogContext" ]
	}
}